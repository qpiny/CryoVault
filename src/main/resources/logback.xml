<configuration>
	<appender name="FILE" class="ch.qos.logback.core.FileAppender">
		<file>log/cryo.log</file>

		<encoder>
			<pattern>
				%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n
			</pattern>
		</encoder>
	</appender>

	<logger name="org.apache.http" level="INFO" />

	<root level="INFO">
		<appender-ref ref="FILE" />
	</root>
</configuration>
